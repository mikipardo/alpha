<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ProyectSkills</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PrimerServlet</servlet-name>
    <servlet-class>alpha.data.servlets.ServletCar</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>PrimerServlet</servlet-name>
    <url-pattern>/ServletCar</url-pattern>
</servlet-mapping>
<!-- Dos servlet que se llaman uno al otro -->
  <servlet>
    <servlet-name>ServletCall</servlet-name>
    <servlet-class>alpha.data.servlets.ServletCall1</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletCall</servlet-name>
    <url-pattern>/llamaServlet</url-pattern>
</servlet-mapping>
  <servlet>
    <servlet-name>ServletReception</servlet-name>
    <servlet-class>alpha.data.servlets.ServletCall2Reception</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletReception</servlet-name>
    <url-pattern>/recibeServlet</url-pattern>
</servlet-mapping>
</web-app>